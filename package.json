    "cordova-plugin-contacts": "^3.0.1",
    "cordova-plugin-dialer": "^0.5.7",
    "cordova-universal-clipboard": "^0.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-thunk-it": "^1.0.7",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "phonegap": "^8.0.0",
    "react-app-rewired": "^1.5.2",
    "source-map-explorer": "^1.5.0"
  },
  "cordova": {
    "plugins": {
